/* CSS PARA PÁGINA TODAS AS PÁGINAS EM GERAL */
	
	// EMPURRA A DIV DE CONTEÚDO PARA DEPOIS DO MENU TOP
.LimiteTopMargin
	margin-bottom 83px
.MarginContent
	margin-bottom: 49px;
.popover.fade
	background-color CorAzul1
	color Corbranca
	border none
	border-radius 0
	padding 14px 14px 15px 14px
	text-align center
	FontExoRegular(normal)
	.arrow
		border none
		&:after
			border-top-color CorAzul1

// ESTILO DA PÁGINA DE AMIGOS DA SAÚDE
.bodyAmSaude
	background-image url("../imagens/background/background-amigosSaude.jpg")
	background-repeat no-repeat
	background-position center
	background-size 411px
.PgAmigosSaude
	margin-top -14px
	.page-header
		margin-top 0
		margin-bottom 0
		padding-bottom 18px	
	.fixoBottom
		position fixed
		bottom 0
		left 0
		width 100%
		padding-left 8.33333333%
		padding-right 8.33333333%
		margin-bottom 49px

// ESTILO DA PÁGINA DE FORUM
.ContentForum
	.col-xs-6
		padding 4px
		.thumbnail
			border-radius 0
			padding 0
			min-height 186px
			margin 0
			box-shadow none
			border none
			cursor pointer
		.thumbSaude
			background transparent url("../imagens/background/background-saude.jpg") no-repeat center
			background-size cover
			span
				// NESSA FUNÇÃO SE COLOCA NOME DA IMAGEM, LARGURA, ALTURA E TAMANHO DA FONTE
				spanIcon("CoracaoWN.png",89px,87px,25px)
		.thumbAlimentacao
			background transparent url("../imagens/background/background-alimentacao.jpg") no-repeat center
			background-size cover
			span
				spanIcon("ComidaWN.png",143px,87px,22px)
		.thumbEsporte
			background transparent url("../imagens/background/background-esporte.jpg") no-repeat center
			background-size cover
			span
				spanIcon("EsporteWN.png",146px,112px,16px)
		.thumbBemEstar
			background transparent url("../imagens/background/background-bemestar.jpg") no-repeat center
			background-size cover
			span
				spanIcon("SorrisoWN.png",119px,87px,21px)
		&:hover
		&:active
		&:focus
			-ms-filter "progid:DXImageTransform.Microsoft.Alpha(Opacity=50)"			
			-moz-opacity 0.5
			-khtml-opacity 0.5
			opacity 0.5

// PARTE DE FEED
.LinkNextFeed
.LinkPreviousFeed
	position fixed
	font-size 58px
	z-index 1031
	color CorCinza2
	bottom 36px
	display block
	height 59px
	width 39px
	left 50%
	cursor pointer
	margin-left calc( "-39px / 2" )
	opacity 0.51
	-ms-filter "progid:DXImageTransform.Microsoft.Alpha(Opacity=51)"
	-moz-opacity 0.51
	-khtml-opacity 0.51
	&:hover
	&:active
	&:focus
		color CorCinza2
		opacity 1
		-ms-filter "progid:DXImageTransform.Microsoft.Alpha(Opacity=100)"
		-moz-opacity 1
		-khtml-opacity 1
	span
		display none

.LinkPreviousFeed
	bottom initial
	top 25px


.ContentFeed
	.col-xs-12
		padding 5px
		margin-top -20px
		.thumbnail
			padding 0
			padding-bottom 5px
			border-radius 0
			box-shadow none
			border none
			margin-bottom 18px
			
		.ComImagem
		.SemImagem
		.Evento			
			.ImagemDestaque
				background #d5d5d5 url("../imagens/Feed/Fundo-Imagem.jpg") no-repeat center
				background-size cover
				width 100%
				min-height 252px
				max-height 252px
				height 100%
				.backdrop				
					content ''
					width calc( "100% - 10px" )
					max-height 252px
					height 100%
					position absolute
					background-color #000
					opacity 0.51
					-ms-filter "progid:DXImageTransform.Microsoft.Alpha(Opacity=51)"
					-moz-opacity 0.51
					-khtml-opacity 0.51
			h3
				position relative				
				float left
				display block
				width calc( "100% - 32px" )
				max-height 53px
				height 100%
				overflow hidden
				display -webkit-box
				-webkit-line-clamp 2
				-webkit-box-orient vertical
				margin-top -65px
				margin-left 16px
				z-index 3
				color Corbranca
				FontExoLight(normal)
			.caption
				p
					font-size rem( 23px )
			.footerThumb
				margin 1px 0 -3px 0
				line-height 38px
				border-top 1px solid #ccc
				a
					&:hover
					&:focus					
						text-decoration none
						outline 0
						color CorAzulFacebook2
				.Curtidas
					a
						color CorVerde2
						&:hover
							color CorVerde2
				.form-group
					margin-bottom 0
					.checkbox-item
						width 100%
						label
							font-size 16px	
					.checkbox	
						input	
							& + span
								&:after
									border-bottom 3px solid Corbranca
									border-right 3px solid Corbranca
		.SemImagem
			h3
				margin-top 16px
				color CorCinza2
				height auto
				max-height none
				overflow initial
				display block
			.caption
				p
					margin-left 8px
		.Evento			
			.ImagemDestaque
				min-height 190px
				max-height 190px
				.backdrop
					max-height 190px
				.DataEvento
				.InfEvento
					float left
					span
					h2
						FontExoLight(normal)
						color Corbranca
						display block
						position relative
						margin 0
						padding 0						
						z-index 3
				.DataEvento
					width 98px
					margin-left 13px
