			.caption
				padding-left 21px
				padding-right 21px
				p
					font-size 18px
			.footerThumb
				margin 1px 0 -3px 0
				line-height 38px
				border-top 1px solid #ccc
				height 56px
				button.btn
					margin-top 22px
		.Anuncio
			.caption
				small
					color CorCinza6
					display block
